<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Demostart" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/grey_90</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="font">@font/sf_pro_display_regular</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <item name="android:windowLightStatusBar" tools:targetApi="23">true</item>
        <!-- Customize your theme here. -->
        <item name="actionBarTheme">@color/white</item>
    </style>

    <style name="Theme.Demostart.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Demostart.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.Demostart.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="Theme.SplashScreen" parent="Theme.MaterialComponents.NoActionBar"/>

</resources>